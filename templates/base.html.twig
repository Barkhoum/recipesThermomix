            <div class="dropdown-menu">
                <a class="dropdown-item" href="{{ path('admin')}}">Mon
                    compte</a>
                <div class="dropdown-divider"></div>

            </div>
        </li>
        <div class="d-flex">
            <ul class="navbar-nav me-auto">
                {% if app.user %}
                    <li class="nav-item dropdown" style="margin-right: 5rem;">
                        <a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" href="#" role="button"
                           aria-haspopup="true" aria-expanded="false">{{ app.user.fullName }}</a>
                        <div class="dropdown-menu">

                            <a class="dropdown-item" href="{{ path('app_espace_user', {id : app.user.id}) }}">Mon
                                compte</a>
                            <a class="dropdown-item" href="{{ path('user.edit', {id : app.user.id}) }}">Modifier mes
                                    informations</a>
                                <a class="dropdown-item" href="{{ path('user.edit.password', {id : app.user.id}) }}">Modifier
