                    <a class="nav-link" href="{{ path('ingredient.index') }}">Mes ingredients</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="{{ path('recipe.index') }}">Mes recettes</a>
                </li>
            {% endif %}
            <li class="nav-item">
                <a class="nav-link" href="{{ path('app_contact') }}">Contact</a>
            </li>
